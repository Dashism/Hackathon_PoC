{
    "name": "hackathon",
    "version": "1.0.0",
    "description": "hackathon application implemented in JavaScript",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "keywords": [
        "fabric-client sample app",
        "balance-transfer node sample",
        "v1.0 fabric nodesdk sample"
    ],
    "engines": {
        "node": ">=8.9.4 <9.0",
        "npm": ">=5.6.0 <6.0"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "body-parser": "^1.17.1",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.3",
        "express": "^4.15.2",
        "express-bearer-token": "^2.1.0",
        "express-jwt": "^5.1.0",
        "express-session": "^1.15.2",
        "fabric-ca-client": "~1.4.0",
        "fabric-client": "~1.4.0",
        "fabric-network": "~1.4.0",
        "fs-extra": "^2.0.0",
        "jsonwebtoken": "^7.3.0",
        "log4js": "^0.6.38"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
